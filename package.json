{
  "name": "@metaquanta/template",
  "version": "0.0.1",
  "description": "",
  "homepage": "https://metaquanta.com",
  "license": "AGPL-3.0",
  "repository": "github:metaquanta/",
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "build:watch": "tsc -w -b ./tsconfig.json",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "lint": "eslint --ext=ts,js src",
    "format": "prettier --write *",
    "serve": "parcel src/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "npm-run-all -s typecheck lint format test"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^2.1.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.0"
  }
}
